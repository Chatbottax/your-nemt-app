{
  "name": "your-nemt-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "server": "nodemon server/index.js",
    "frontend": "next dev",
    "build": "next build",
    "start": "concurrently \"node server/index.js\" \"next start\"",
    "db:migrate": "prisma migrate deploy",
    "db:sqlite:push": "prisma db push --schema prisma/schema.sqlite.prisma",
    "db:sqlite:generate": "prisma generate --schema prisma/schema.sqlite.prisma",
    "db:sqlite:reset": "node -e \"const fs=require('fs');try{fs.unlinkSync('prisma/dev.db')}catch(e){}\" && npm run db:sqlite:push && npm run db:sqlite:generate && npm run seed",
    "dev:sqlite": "npm run db:sqlite:push && npm run db:sqlite:generate && npm run seed && npm run dev",
    "dev:sqlite:quick": "node scripts/init-sqlite.js && npm run db:sqlite:generate && npm run seed && npm run dev",
    "seed": "node prisma/seed.js",
    "test": "echo 'No tests'"
  },
  "dependencies": {
    "@prisma/client": "^5.2.0",
    "autoprefixer": "^10.4.14",
    "pdf-parse": "^1.1.1",
    "better-sqlite3": "^12.2.0",
    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "multer": "^1.4.5-lts.1",
    "next": "15.5.2",
    "node-fetch": "^3.3.1",
    "postcss": "^8.4.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "prisma": "^5.2.0"
  }
}
